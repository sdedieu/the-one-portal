{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"the-one-portal-shell": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/the-one-portal-shell",
			"sourceRoot": "projects/the-one-portal-shell/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/the-one-portal-shell",
						"index": "projects/the-one-portal-shell/src/index.html",
						"main": "projects/the-one-portal-shell/src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/the-one-portal-shell/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/the-one-portal-shell/src/favicon.ico",
							"projects/the-one-portal-shell/src/assets"
						],
						"styles": [
							"projects/the-one-portal-shell/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/the-one-portal-shell/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/the-one-portal-shell/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "the-one-portal-shell:build:production",
							"extraWebpackConfig": "projects/the-one-portal-shell/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "the-one-portal-shell:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4210,
						"publicHost": "http://localhost:4210",
						"extraWebpackConfig": "projects/the-one-portal-shell/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "the-one-portal-shell:build",
						"extraWebpackConfig": "projects/the-one-portal-shell/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/the-one-portal-shell/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/the-one-portal-shell/src/favicon.ico",
							"projects/the-one-portal-shell/src/assets"
						],
						"styles": [
							"projects/the-one-portal-shell/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		}
	},
	"cli": {
		"analytics": false
	},
	"schematics": {
		"@angular-eslint/schematics:application": {
			"setParserOptionsProject": true
		},
		"@angular-eslint/schematics:library": {
			"setParserOptionsProject": true
		}
	}
}